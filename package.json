{
    "name": "notella-ui",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "fontello:generate": "rm -r ./.fontello-session & fontello-cli --config ./src/scss/fontello/config.json --css ./src/scss/fontello/css --font ./src/scss/fontello/font install",
        "fontello:open": "rm -r ./.fontello-session & fontello-cli --config ./src/scss/fontello/config.json open",
        "postinstall": "node ./node_modules/frontend-dependencies/index.js"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@toast-ui/editor": "^3.1.1",
        "@vueuse/rxjs": "^6.9.2",
        "dexie": "^3.2.0",
        "events": "^3.3.0",
        "mitt": "^3.0.0",
        "pouchdb": "^7.2.2",
        "pouchdb-adapter-http": "^7.2.2",
        "pouchdb-adapter-idb": "^7.2.2",
        "pouchdb-browser": "^7.2.2",
        "pouchdb-utils": "^7.2.2",
        "rxdb": "^10.5.3",
        "rxjs": "^7.4.0",
        "vue": "^3.2.16",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@vitejs/plugin-vue": "^1.9.3",
        "eslint": "7.32.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "sass": "^1.43.3",
        "vite": "^2.6.4",
        "vite-plugin-pages": "^0.18.1",
        "vite-plugin-windicss": "^1.4.12",
        "windicss": "^3.1.9"
    }
}
